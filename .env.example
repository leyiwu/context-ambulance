# Context Ambulance Configuration
# Copy this file to .env and fill in your API keys

# Analyzer Provider (options: gemini, claude, openai, none)
ANALYZER_PROVIDER=gemini

# API Keys (optional - only needed if using LLM-based analysis)
GOOGLE_API_KEY=your_gemini_api_key_here
ANTHROPIC_API_KEY=your_claude_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Analysis Settings
MAX_MESSAGES_TO_ANALYZE=100
SANITIZATION_LEVEL=balanced  # options: minimal, balanced, aggressive

# Output Settings
OUTPUT_DIR=./rescue_packages
